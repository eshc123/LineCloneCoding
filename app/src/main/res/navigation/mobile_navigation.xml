<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="org.standardj.rine.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_conversation"
        android:name="org.standardj.rine.ui.conversation.ConversationFragment"
        android:label="@string/title_conversation"
        tools:layout="@layout/fragment_conversation" />

    <fragment
        android:id="@+id/navigation_timeline"
        android:name="org.standardj.rine.ui.timeline.TimelineFragment"
        android:label="@string/title_timeline"
        tools:layout="@layout/fragment_timeline" />

    <fragment
        android:id="@+id/navigation_call"
        android:name="org.standardj.rine.ui.call.CallFragment"
        android:label="@string/title_call"
        tools:layout="@layout/fragment_call" />

</navigation>